<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#494343"/>
            <stroke
                android:width="1dp"
                android:color="#000000"/>
            <corners
                android:radius="1200dp"/>
            
            <padding
                android:left="7dp"
                android:top="7dp"
                android:right="7dp"
                android:bottom="7dp" />
        </shape>
   </item>
   <item android:state_pressed="false">
        <shape>
            <solid
                android:color="#EADEC4"/>
            <stroke
                android:width="1dp" 
                android:color="#000000"/>
            <corners
                android:radius="1200dp" />
            <padding
                android:left="7dp"
                android:top="7dp"
                android:right="7dp"
                android:bottom="7dp" />
        </shape>
    </item>
</selector>
